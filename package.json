{
  "name": "trinat-pwa",
  "version": "2.0.0",
  "description": "SIA Trinat local area restaurant in Riga small-scope project. Developed on React, Google Firebase, RaspberryPi and GraphQL API in business purposes",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "test": "eslint \"**/*.js\" --ignore-pattern node_modules/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/winniepukki/trinat"
  },
  "keywords": [],
  "author": "Aleksandrs Bogackins",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.12.5",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-winniepukki-guardian": "^1.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-winniepukki-guidelines": "^2.1.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss-loader": "^4.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "firebase": "^8.1.1",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --cache --no-error-on-unmatched-pattern --ext .js,.ts,.tsx"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@component(.*)$": "<rootDir>/src/app/component$1",
      "^@store(.*)$": "<rootDir>/src/app/store$1",
      "^@util(.*)$": "<rootDir>/src/app/util$1",
      "^@type(.*)$": "<rootDir>/src/app/type$1",
      "^@query(.*)$": "<rootDir>/src/app/query$1"
    }
  }
}
